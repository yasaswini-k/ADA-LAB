#include<stdio.h>
#include<time.h>
void TOH(int n,char x,char y,char z) {
   if(n>0) {
      TOH(n-1,x,z,y);
      printf("\n%c to %c",x,y);
      TOH(n-1,z,y,x);
   }
}
int main() {
   int n;
   time_t start,end;
   printf("enter the number of discs");
   scanf("%d",&n);
   start=time(NULL);
   TOH(n,'A','B','C');
   end=time(NULL);
   printf("Time is %fs",difftime(end,start));
}
